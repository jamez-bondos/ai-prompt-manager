{
  "name": "ai-prompt-manager",
  "version": "1.0.0",
  "description": "AI Prompt Template Manager for macOS",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.js && tsc -p tsconfig-main.json",
    "watch": "webpack --config webpack.config.js --watch",
    "dev": "npm run build && electron .",
    "package": "electron-builder build --mac"
  },
  "keywords": ["AI", "Prompt", "Template", "Manager", "Electron", "macOS"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.3.0",
    "antd": "^5.15.0",
    "@ant-design/icons": "^5.3.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/node": "^20.11.19",
    "electron-reload": "^2.0.0-alpha.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "css-loader": "^6.10.0",
    "style-loader": "^3.3.4",
    "html-webpack-plugin": "^5.6.0"
  },
  "build": {
    "appId": "com.ai-prompt-manager",
    "productName": "AI Prompt Manager",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "release"
    }
  }
}
